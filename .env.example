# WalletConnect Project ID (Required)
VITE_WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id_here

# Supabase Configuration (Required for Analytics)
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# IPFS Configuration (Choose one)
# Option 1: Infura IPFS
VITE_INFURA_PROJECT_ID=your_infura_project_id
VITE_INFURA_PROJECT_SECRET=your_infura_project_secret

# Option 2: Pinata (for pinning)
VITE_PINATA_API_KEY=your_pinata_api_key
VITE_PINATA_SECRET_KEY=your_pinata_secret_key

# Option 3: Web3.Storage
VITE_WEB3_STORAGE_TOKEN=your_web3_storage_token

# Contract Addresses (Update after deployment)
VITE_TOKEN_FACTORY_ADDRESS=0x...
VITE_BADGE_NFT_ADDRESS=0x...
VITE_HYBRID_LIQUIDITY_POOL_ADDRESS=0x...

# Network Configuration
VITE_NETWORK=base # or base-sepolia for testnet
VITE_RPC_URL=https://mainnet.base.org # or testnet RPC

# Analytics (Optional)
VITE_ANALYTICS_API_KEY=your_analytics_key